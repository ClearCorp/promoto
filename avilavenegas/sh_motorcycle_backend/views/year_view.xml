<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<!-- form view -->
	<record id="sh_motorcycle_year" model="ir.ui.view">
		<field name="name">sh.motorcycle.year.form</field>
		<field name="model">motorcycle.year</field>
		<field name="arch" type="xml">
			<form string="Motorcycle Year">
				<sheet>
					<sheet>
						<group>
							<group>
							<field name="name"/>
							</group>
						</group>
					</sheet>
				</sheet>
			</form>
		</field>
	</record>
	
	<!-- Tree View -->
	<record id="sh_motorcycle_year_tree" model="ir.ui.view">
		<field name="name">sh.motorcycle.year.tree</field>
		<field name="model">motorcycle.year</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name"/>
			</tree>
		</field>
	</record>
	
	<!-- MenuAction -->
	<record id="sh_motorcycle_year_action" model="ir.actions.act_window">
		<field name="name">AÃ±o</field>
		<field name="res_model">motorcycle.year</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
	</record>
	
	
	<!-- Menu Item -->
	<menuitem id="sh_motorcycle_year_menuitem"
			  action="sh_motorcycle_year_action"
			  parent="sh_motorcycle_menuitem_config"
			  sequence="4"
			  />



</odoo>
